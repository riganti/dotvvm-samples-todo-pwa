{
  "compilerOptions": {
    "baseUrl": "./",
    "module": "commonjs",
    "lib": [ "dom", "es5", "es2015.core", "es2015.promise" ],
    "strictNullChecks": false,
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es6"
  },
  "compileOnSave": false,
  "exclude": [
    "node_modules",
    "Scripts/typings",
    "Scripts/ServiceWorker"
  ]
}